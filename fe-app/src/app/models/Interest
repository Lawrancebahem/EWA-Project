export class Interest {

    constructor(private name:string, private id:number) {

    }

    public static trueCopy(interest: Interest): Interest {

        return Object.assign(new Interest("****",0), interest)
    }


    get id(): number {
        return this._id;
    }

    set id(value: number) {
        this._id = value;
    }

    get name() {
        return this._name;
    }

    set name(value) {
        this._name = value;
    }
}
